{
    "pkg_managers": {
        "apt-get": "apt-get update -y",
        "dnf": "dnf upgrade -y",
        "yum": "yum check-update -y",
        "pacman": "pacman -Syu --noconfirm",
        "brew": "brew update"
    },
    "nvim_deps": {
        "node_packages": [
            "neovim"
        ],
        "pip_packages": [
            "pynvim"
        ]
    },
    "relative_paths": [
        ".zshrc",
        ".config/nvim",
        ".tmux.conf"
    ],
    "plugins": {
        "zsh-syntax-highlighting": "https://github.com/zsh-users/zsh-syntax-highlighting.git",
        "zsh-autosuggestions": "https://github.com/zsh-users/zsh-autosuggestions.git",
        "zsh-history-substring-search": "https://github.com/zsh-users/zsh-history-substring-search.git"
    },
    "tmux_plugins": {
        "tpm": "https://github.com/tmux-plugins/tpm.git",
        "dracula": "https://github.com/dracula/tmux.git"
    },
    "common_packages": [
        "git",
        "tmux",
        "wget",
        "fontconfig"
    ],
    "distro_packages": {
        "arch": [
            "lua",
            "xclip",
            "tree-sitter",
            "tree-sitter-cli",
            "unzip",
            "ripgrep",
            "fd"
        ],
        "debian": [
            "lua5.4",
            "xclip",
            "unzip",
            "ripgrep",
            "fd-find",
            "python3-venv",
            "curl",
            "locales"
        ],
        "fedora": [
            "lua",
            "ripgrep",
            "fd-find",
            "python3-devel",
            "glibc-locale-source",
            "glibc-langpack-en"
        ],
        "ubuntu": [
            "lua5.4",
            "ripgrep",
            "fd-find",
            "python3-venv",
            "locales"
        ],
        "macos": [
            "lua",
            "ripgrep",
            "fd"
        ]
    },
    "term_color:": "xterm-256color",
    "oh_my_zsh_custom_theme_repo": "romkatv/powerlevel10k",
    "font_name": "MesloLGS NF",
    "nvim_profiles": {
        "default": {
            "checks": {
                "required": [
                    "node"
                ],
                "one_of": [
                    "npm",
                    "yarn",
                    "pnpm"
                ]
            },
            "parsers": [
                "markdown_inline"
            ],
            "lsps": [
                "lua-language-server"
            ],
            "daps": [],
            "linters": [],
            "formatters": [
                "stylua"
            ]
        },
        "golang": {
            "checks": {
                "required": [
                    "go"
                ]
            },
            "lsps": [
                "gopls"
            ],
            "daps": [
                "go-debug-adapter"
            ],
            "linters": [],
            "formatters": [
                "goimports-reviser",
                "golines",
                "gomodifytags",
                "gotests"
            ],
            "custom_script": "golang.sh"
        },
        "typescript": {
            "checks": {
                "required": [
                    "node"
                ],
                "one_of": [
                    "npm",
                    "yarn",
                    "pnpm"
                ]
            },
            "lsps": [
                "typescript-language-server"
            ],
            "daps": [],
            "linters": [
                "eslint_d"
            ],
            "formatters": [
                "prettierd"
            ]
        },
        "python": {
            "checks": {
                "required": [
                    "python"
                ]
            },
            "lsps": [
                "pyright"
            ],
            "daps": [],
            "linters": [],
            "formatters": [
                "black"
            ]
        }
    }
}